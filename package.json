{
  "name": "zemizer-translator",
  "displayName": "ZeMizer Translator",
  "publisher": "Kmzx",
  "description": "A simple translator for VS Code.",
  "author": {
    "name": "ZeKmzx"
  },
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "zemizer",
    "translate",
    "translator",
    "trad",
    "translation"
  ],
  "icon": "images/icon2.png",
  "pricing": "Free",
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zemizer-translator.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "zemizer-translator.translate",
        "title": "ZeTranslate"
      },
      {
        "command": "zemizer-translator.tr1",
        "title": "ZeTr1",
        "properties": {
          "zemizer-translator.tr1": {
            "description": "Active ou désactive une fonctionnalité de ma super extension."
          }
        }
      },
      {
        "command": "zemizer-translator.tr2",
        "title": "ZeTr2",
        "properties": {
          "zemizer-translator.tr2": {
            "type": "boolean",
            "default": true,
            "description": "Active ou désactive une fonctionnalité de ma super extension."
          }
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "language-tags": "^1.0.9"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaidouni/vsc.ext.translator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlaidouni/vsc.ext.translator.git"
  },
  "homepage": "https://github.com/mlaidouni/vsc.ext.translator/blob/master/README.md"
}